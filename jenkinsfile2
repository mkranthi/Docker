pipeline {

    agent any
    stages {
        stage('Code Checkout') {
            steps {
                git url: 'https://github.com/mkranthi/Docker.git'
            }
        }
        stage('login into jfrog artifactory'){
            steps{
                sh "docker login https://devcompute.jfrog.io -u admin -p Devops@1"
            }
        }
        stage('pullimage from jfrog artifactory'){
            steps{
                sh "docker pull devcompute.jfrog.io/itsoli-docker-develop-local/demo3-image:latest"
            }
        }
        stage('create pod'){
            steps{
                sh "kubectl create -f sample-pod.yaml"
            }
        }
    }
}
